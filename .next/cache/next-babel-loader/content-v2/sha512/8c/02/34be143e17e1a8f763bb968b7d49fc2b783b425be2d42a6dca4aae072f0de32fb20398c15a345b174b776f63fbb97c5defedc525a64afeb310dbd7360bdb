ggle ? (\r\n                <Container className=\"ToogleTooltip\">\r\n                  <Row>\r\n                    <Col xs={4} md={4} lg={4}>\r\n                      <label>Adultos</label>\r\n                    </Col>\r\n                    <Col xs={6} md={6} lg={6}>\r\n                      <Row>\r\n                        <Button\r\n                          color=\"danger\"\r\n                          onClick={this.decrementAdults}\r\n                          className=\"no-radius-1\"\r\n                        >\r\n                          -\r\n                        </Button>\r\n                        <div className=\"tooltip-Number\">\r\n                          {this.state.adults}\r\n                        </div>\r\n                        <Button\r\n                          color=\"success\"\r\n                          onClick={this.incrementAdults}\r\n                          className=\"no-radius-2\"\r\n                        >\r\n                          +\r\n                        </Button>\r\n                      </Row>\r\n                    </Col>\r\n                  </Row>\r\n                  <Row className=\"mt-2\">\r\n                    <Col xs={4} md={4} lg={4}>\r\n                      <label>Menores</label>\r\n                    </Col>\r\n                    <Col xs={6} md={6} lg={6}>\r\n                      <Row>\r\n                        <Button\r\n                          color=\"danger\"\r\n                          onClick={this.decrementChilds}\r\n                          className=\"no-radius-1\"\r\n                        >\r\n                          -\r\n                        </Button>\r\n                        <div className=\"tooltip-Number\">\r\n                          {this.state.childs}\r\n                        </div>\r\n                        <Button\r\n                          color=\"success\"\r\n                          onClick={this.incrementChilds}\r\n                          className=\"no-radius-2\"\r\n                        >\r\n                          +\r\n                        </Button>\r\n                      </Row>\r\n                    </Col>\r\n                  </Row>\r\n                </Container>\r\n              ) : (\r\n                \"\"\r\n              )}\r\n            </Container>\r\n          </Col>\r\n        </Row>\r\n        <Row className=\"pl-2 pr-2 pb-4\">\r\n          <Col xs={12} md={12} lg={12}>\r\n            <Button color=\"info\" className=\"col-sm-10 offset-sm-1\">\r\n              Reservar\r\n            </Button>\r\n          </Col>\r\n        </Row>\r\n      </>\r\n    );\r\n  }\r\n}\r\n\r\nconst styles = {\r\n  input: {\r\n    border: \"1px solid #ccc\",\r\n    padding: \"5px\",\r\n  },\r\n};\r\n\r\nexport default AvailabilityInput;\r\n"]},"metadata":{},"sourceType":"module"}